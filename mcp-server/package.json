{
  "name": "snipeit-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for Snipe-IT asset management",
  "type": "module",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "build:http": "tsc -p tsconfig.http.json",
    "watch": "tsc --watch",
    "start": "export $(grep -v '^#' .env | grep -v '^$' | xargs) && node build/index.js",
    "start:http": "export $(grep -v '^#' .env | grep -v '^$' | xargs) && node build-http/index.js",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "pretest": "npm run audit"
  },
  "keywords": [
    "mcp",
    "snipeit",
    "asset-management"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "axios": "^1.13.5",
    "express": "^5.2.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^20.0.0",
    "typescript": "^5.9.3"
  }
}
